{
  "name": "jax-js-bayes",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "dependencies": {
    "jax-js-mcmc-2": "github:StefanSko/jax-js-mcmc-2"
  },
  "peerDependencies": {
    "@jax-js/jax": ">=0.1.0",
    "@observablehq/plot": ">=0.6"
  },
  "peerDependenciesMeta": {
    "@observablehq/plot": {
      "optional": true
    }
  },
  "devDependencies": {
    "adm-zip": "^0.5.12",
    "@types/node": "^20.12.7",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:distributions": "vitest run tests/distributions",
    "test:constraints": "vitest run tests/constraints",
    "test:posteriordb": "vitest run tests/posteriordb",
    "test:posteriordb:mem": "POSTERIORDB_MEM_LOG=1 POSTERIORDB_MEM_GC=1 POSTERIORDB_MEM_DISPOSE=1 NODE_OPTIONS=\"--expose-gc --max-old-space-size=8192\" vitest run tests/posteriordb",
    "test:posteriordb:isolated": "bash tools/run-posteriordb-isolated.sh",
    "memory:repro-hmc": "HMC_MEM_REPRO=1 NODE_OPTIONS=\"--expose-gc --max-old-space-size=4096\" vitest run tests/memory/hmc-repro.test.ts",
    "memory:profile-mcmc2": "bash tools/run-mcmc2-memory-profile.sh"
  },
  "pnpm": {
    "packageExtensions": {
      "jax-js-mcmc-2@*": {
        "exports": {
          ".": "./src/index.ts"
        },
        "main": "./src/index.ts",
        "module": "./src/index.ts"
      }
    }
  }
}
